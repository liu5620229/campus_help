A,IADA,CACA,UAAA,EAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,EAAA,CAAA,IAAA,CAAA,IAAA,CADA,CAEA;AACA,OAJA;AAKA,KAnCA;AAoCA,IAAA,SApCA,qBAoCA,IApCA,EAoCA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAvCA,GAFA;AA2CA,EAAA,IA3CA,kBA2CA;AACA,WAAA;AACA;AACA,MAAA,OAAA,EAAA,EAFA;AAGA,MAAA,KAAA,EAAA,EAHA;AAIA,MAAA,QAAA,EAAA;AACA,QAAA,EAAA,EAAA,CADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAJA;AAQA,MAAA,OAAA,EAAA,CACA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAFA,CARA;AAaA,MAAA,KAAA,EAAA,EAbA;AAcA,MAAA,MAAA,EAAA;AAdA,KAAA;AAgBA,GA5DA;AA6DA,EAAA,OA7DA,qBA6DA;AAAA;;AACA,SAAA,OAAA;AACA,SAAA,IAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,KAHA;AAIA,GAnEA;AAqEA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAJA;AArEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <!-- <div class=\"center\">\r\n            <el-input placeholder=\"请输入内容\" v-model=\"input\" class=\"input-with-select\" @keydown.enter.native=\"clickSearch\">\r\n                <el-select v-model=\"select\" slot=\"prepend\" placeholder=\"请选择\" value=\"1\" @change=\"inquiry\">\r\n                    <el-option value=\"entire\" label=\"全部\"></el-option>\r\n                    <el-option :value=\"item.id\" v-for=\"item in schools\" :label=\"item.name\"></el-option>\r\n                </el-select>\r\n                <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"clickSearch\"></el-button>\r\n            </el-input>\r\n        </div> -->\r\n        <div class=\"bottom\">\r\n            <el-table\r\n                    :data=\"tasks\"\r\n                    :resizable=\"false\"\r\n                    style=\"width: 100%  \">\r\n                <el-table-column\r\n                        prop=\"publish.username\"\r\n                        label=\"发布人\"\r\n                        min-width=\"140\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                        label=\"接受人\"\r\n                        min-width=\"140\">\r\n                    <template slot-scope=\"scope\">\r\n                        {{scope.row.accept ? scope.row.accept.username : '暂无服务'}}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                        prop=\"reward\"\r\n                        label=\"任务奖励\"\r\n                        min-width=\"110\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                        prop=\"school.name\"\r\n                        label=\"任务所在学校\"\r\n                        min-width=\"200\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                        prop=\"taskTitle\"\r\n                        label=\"标题\"\r\n                        min-width=\"180\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                        label=\"发布时间\"\r\n                        min-width=\"160\">\r\n                    <template slot-scope=\"scope\">\r\n                        {{scope.row.createTime ? transform(scope.row.createTime) : '暂无时间'}}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                        label=\"服务时间\"\r\n                        min-width=\"160\">\r\n                    <template slot-scope=\"scope\">\r\n                        {{scope.row.orderTime ? transform(scope.row.orderTime) : '暂无时间'}}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                        prop=\"balance\"\r\n                        label=\"完成时间\"\r\n                        min-width=\"160\">\r\n                    <template slot-scope=\"scope\">\r\n                        {{scope.row.endTime ? transform(scope.row.endTime) : '暂无时间'}}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                        label=\"任务状态\"\r\n                        min-width=\"90\">\r\n                    <template slot-scope=\"scope\">\r\n                        {{scope.row.state == 0?'未处理':scope.row.state == 1 ? '处理中' : '已完成'}}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                        label=\"操作\"\r\n                        width=\"100\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-popconfirm\r\n                                confirm-button-text='好的'\r\n                                cancel-button-text='不用了'\r\n                                icon=\"el-icon-info\"\r\n                                icon-color=\"red\"\r\n                                title=\"确定删除该任务吗？\"\r\n                                @confirm=\"del(scope.row.id,scope.row.state)\"\r\n                        >\r\n                            <el-button type=\"text\" size=\"small\" slot=\"reference\"><i class=\"el-icon-delete\" style=\"color: red\"></i></el-button>\r\n                        </el-popconfirm>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import {formatDate} from '@/util/date';\r\n    export default {\r\n        name: \"Task\",\r\n        methods: {\r\n            inquiry(){\r\n                if (this.select == 'entire'){\r\n                    this.newList();\r\n                }else {\r\n                    this.$post(\"/task/api/findTaskBySchoolId\",{\"id\":this.select})\r\n                    .then(res => {\r\n                        this.tasks = res.data.tasks\r\n                    })\r\n                }\r\n            },\r\n            clickSearch() {\r\n                if(this.input == '已完成'){\r\n\r\n                }\r\n            },\r\n\r\n            del(id,state) {\r\n                if (state == 0){\r\n                    this.$del(\"/task/\"+id)\r\n                    .then((res) => {\r\n                        this.$notifyMsg(\"成功\", res.data.msg, \"success\")\r\n                        this.newList()\r\n                    })\r\n                }else {\r\n                    this.$msg(\"该任务已被接收或已完成\",\"error\")\r\n                }\r\n            },\r\n\r\n            newList() {\r\n                this.$get(\"/task\")\r\n                .then((rs) => {\r\n                    this.tasks = rs.data.task\r\n                    // console.log(this.tasks)\r\n                })\r\n            },\r\n            transform(time) {\r\n                let date = new Date(time);\r\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\r\n            }\r\n        },\r\n        data() {\r\n            return {\r\n                //学校\r\n                schools:[],\r\n                tasks: [],\r\n                ruleForm: {\r\n                    id: 0,\r\n                    state: true\r\n                },\r\n                options: [\r\n                    {label: \"正常\", value: \"0\"},\r\n                    {label: \"禁用\", value: \"1\"}\r\n\r\n                ],\r\n                input:\"\",\r\n                select: 'entire'\r\n            }\r\n        },\r\n        created() {\r\n            this.newList();\r\n            this.$get(\"school\")\r\n            .then(res => {\r\n                this.schools = res.data.schools\r\n            })\r\n        },\r\n\r\n        filters: {\r\n            formatDate(time) {\r\n                let date = new Date(time);\r\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\r\n            }\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n    .content {\r\n        padding: 0 1%;\r\n\r\n    }\r\n\r\n    .center {\r\n        width: 80%;\r\n        margin-bottom: 30px;\r\n    }\r\n\r\n    /deep/ .el-select .el-input {\r\n        width: 200px;\r\n    }\r\n\r\n    /deep/ .input-with-select .el-input-group__prepend {\r\n        background-color: #fff;\r\n    }\r\n\r\n\r\n\r\n    .form {\r\n        margin: 0 22px;\r\n    }\r\n</style>"],"sourceRoot":"src/views/admin/children"}]}
AA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,gBAAA;AACA,aANA;AAOA,WAVA,MAUA;AACA,YAAA,IAAA;AACA;AACA;AApBA,OAAA,EAqBA,KArBA,CAqBA,YAAA,CACA,CAtBA;AAuBA;AAzBA,GARA;AAoCA,EAAA,QAAA,oBACA,QAAA,CAAA,MAAA,EAAA,CAAA,MAAA,CAAA,CADA,CApCA;AAuCA,EAAA,OAvCA,qBAuCA;AAAA;;AACA;AACA,SAAA,IAAA,CAAA,OAAA,EAAA;AAAA,MAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA;AAKA,GA9CA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAJA;AAhDA,CAAA","sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <el-alert\r\n                :title=\"user.school.name + '- - -任务平台共' + tasks.length + '个任务'\"\r\n                :closable=\"false\"\r\n                type=\"info\">\r\n        </el-alert>\r\n\r\n        <div>\r\n            <el-card class=\"box-card\">\r\n                <div slot=\"header\" class=\"clearfix\">\r\n                    <span>任务</span>\r\n                    <el-button style=\"float: right; padding: 3px 0\" type=\"text\">最新</el-button>\r\n                </div>\r\n                <el-card class=\"box-card\" v-for=\"item in tasks\" v-if=\"item.state == 0\">\r\n                    <div slot=\"header\" class=\"clearfix\"\r\n                         style=\"display: flex; align-items: center; justify-content: space-between\">\r\n                        <span>\r\n                            <el-tag type=\"success\" style=\"margin-right: 5px\">待解决</el-tag>\r\n                            {{item.taskTitle}}\r\n                            <i class=\"el-icon-money\" style=\"margin-left: 20px;color: red;\"> {{item.reward}}元</i>\r\n                        </span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\"\r\n                                   v-show=\"user.id != item.publish.id\" @click=\"acceptTask(item.id)\">接受任务\r\n                        </el-button>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\"\r\n                                   v-show=\"user.id == item.publish.id\">本人任务\r\n                        </el-button>\r\n                    </div>\r\n                    <div class=\"text item\">\r\n                        <p class=\"el-icon-s-custom\">{{item.publish.username}}<span style=\"margin-left: 10px;\">{{item.taskContext}}</span>\r\n                        </p>\r\n                        <span style=\"float: right\">{{item.createTime | formatDate}}</span>\r\n                    </div>\r\n                </el-card>\r\n                <div style=\"text-align: center\" v-if=\"tasks.length == 0\">\r\n                    <span><i class=\"el-icon-refresh-right\"></i>暂无任务</span>\r\n                </div>\r\n\r\n            </el-card>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import {mapMutations, mapState} from \"vuex\";\r\n    import {formatDate} from '@/util/date';\r\n\r\n    export default {\r\n        name: \"Index\",\r\n        data() {\r\n            return {\r\n                tasks: [],\r\n            }\r\n        },\r\n\r\n        methods: {\r\n            acceptTask(id) {\r\n                this.$msgbox({\r\n                    title: '提示',\r\n                    message: '确定接受此任务吗？',\r\n                    showCancelButton: true,\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    beforeClose: ((action, instance, done) => {\r\n                        if (action == 'confirm') {\r\n                            // instance.confirmButtonLoading = true;\r\n                            instance.confirmButtonText = '执行中...';\r\n                            this.$put('task/takerAccept', {id: id, \"acceptId\": this.user.id})\r\n                                .then((res) => {\r\n                                    done();\r\n                                    instance.confirmButtonLoading = false;\r\n                                    this.$msg(res.data.msg, \"success\");\r\n                                    this.$router.push('/home/accepted');\r\n                                })\r\n                        } else {\r\n                            done();\r\n                        }\r\n                    })\r\n                }).catch(() => {\r\n                })\r\n            },\r\n        },\r\n\r\n        computed: {\r\n            ...mapState('user', ['user'])\r\n        },\r\n        created() {\r\n            // console.log(this.user)\r\n            this.$get(\"/task\", {id: this.user.id})\r\n                .then((res) => {\r\n                    this.tasks = res.data.task\r\n            })\r\n\r\n        },\r\n\r\n        filters: {\r\n            formatDate(time) {\r\n                let date = new Date(time);\r\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n    .content {\r\n        background: #FFf;\r\n        margin: 0 15px;\r\n        padding: 15px;\r\n\r\n        .card h2 {\r\n            font-weight: normal;\r\n            font-size: 18px;\r\n\r\n            span {\r\n                font-size: 12px;\r\n                display: inline-block;\r\n                border: 1px solid red;\r\n                padding: 1px 3px;\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n    /deep/ .el-alert--info.is-light {\r\n        height: 50px;\r\n    }\r\n\r\n    /deep/ .el-select .el-input {\r\n        width: 130px;\r\n    }\r\n\r\n    /deep/ .input-with-select .el-input-group__prepend {\r\n        background-color: #fff;\r\n    }\r\n\r\n    /deep/ .el-card {\r\n        margin-bottom: 20px;\r\n    }\r\n</style>"],"sourceRoot":"src/views/user/children"}]}